{
	"name": "backend",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"start": "node -r module-alias/register dist/index.js",
		"dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
		"build": "rimraf dist && tsc",
		"typeorm": "node --require ts-node/register --require tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/infrastructure/database/data-source.ts",
		"migration:run": "npm run typeorm migration:run",
		"migration:revert": "npm run typeorm migration:revert",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"seed": "ts-node -r tsconfig-paths/register src/infrastructure/database/seeds/run-seeds.ts",
		"seed-nec": "ts-node -r tsconfig-paths/register src/infrastructure/database/seeds/nec-seeds/index.ts"
	},
